{
  "name": "angular2-base-components",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellgate75/angular2-base-components.git"
  },
  "author": "Fabrizio Torelli <hellgate75@gmail.com>",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "json-server-dev": "json-server --watch --config ./json-server-dev.json ./json-server/all-data-dev.json",
    "json-server-prod": "json-server --watch --config ./json-server-prod.json ./json-server/all-data-prod.json",
    "json-server-test": "json-server --watch --config ./json-server-test.json ./json-server/all-data-test.json",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && ng build -o dist -e dev -dev",
    "build-prod": "rm -rf dist-prod && mkdir dist-prod && ng build -o dist-prod  -e prod -prod",
    "start": "concurrently --names \"json-server-dev,http-dev\" \"npm run json-server-dev\" \"ng serve -w -lr --host 0.0.0.0 --port 4201 --live-reload-port 49153  -e dev -dev\"",
    "prod": "concurrently --names \"json-server-prod,http-prod\" \"npm run json-server-prod\" \"ng serve -o dist --host 0.0.0.0 --port 4200  -e prod -prod\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "concurrently --names \"json-server-test,protractor\" \"npm run json-server-test\" \"protractor\""
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.1.0",
    "base-64": "^0.1.0",
    "bootstrap": "^4.0.0-alpha.3",
    "bootstrap-sass": "^3.3.7",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "graceful-fs": "^4.1.0",
    "minimatch": "^3.0.2",
    "rxjs": "5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.1",
    "@types/jasmine": "^2.2.30",
    "@types/jquery": "^2.0.34",
    "@types/lodash": "^4.14.44",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.20-4",
    "angular-cli-env": "^0.2.0",
    "codelyzer": "~1.0.0-beta.3",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "json-server": "^0.9.4",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
